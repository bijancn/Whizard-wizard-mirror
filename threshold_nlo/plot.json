    "expression": [
      "def data_abs_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v = np.vectorize(include.abs_v_of_sqrts)",
      "  v = v(sqrts)",
      "  return ('abs_v_of_sqrts', np.vstack([sqrts, v]))",
      "",
      "def data_real_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v = np.vectorize(include.real_v_of_sqrts)",
      "  v = v(sqrts)",
      "  return ('real_v_of_sqrts', np.vstack([sqrts, v]))",
      "",
      "def data_imag_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v = np.vectorize(include.imag_v_of_sqrts)",
      "  v = v(sqrts)",
      "  return ('imag_v_of_sqrts', np.vstack([sqrts, v]))",
      "",
      "def data_switchoff_of_real_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v1 = 0.1",
      "  v2 = 0.3",
      "  v = np.vectorize(include.real_v_of_sqrts)",
      "  v = v(sqrts)",
      "  f = np.vectorize(lambda x: include.switch_off(v1,v2,x))",
      "  f = f(v)",
      "  return ('switchoff_of_real_v_of_sqrts', np.vstack([sqrts, f]))",
      "",
      "def data_switchoff_of_imag_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v1 = 0.1",
      "  v2 = 0.3",
      "  v = np.vectorize(include.imag_v_of_sqrts)",
      "  v = v(sqrts)",
      "  f = np.vectorize(lambda x: include.switch_off(v1,v2,x))",
      "  f = f(v)",
      "  return ('switchoff_of_imag_v_of_sqrts', np.vstack([sqrts, f]))",
      "",
      "def data_switchoff_of_abs_v_of_sqrts():",
      "  import numpy as np",
      "  sqrts = np.arange(300.0, 400.0, 0.1)",
      "  v1 = 0.1",
