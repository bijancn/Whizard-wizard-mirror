{
  "processes": [
  { "process": ["NLO", "NLO_noboost"],
    "disabled": true,
    "purpose": "scan",
    "whizard_options": "--no-banner -r",
    "nlo_type": "nlo",
    "scans": [{
      "scan_object": "sqrts",
      "ranges": [
      { "type": "linear",
        "start": 300.0,
        "stop": 330.0,
        "stepsize": 5.0
      },
      { "type": "linear",
        "start": 330.0,
        "stop": 340.0,
        "stepsize": 1.0
      },
      { "type": "linear",
        "start": 340.0,
        "stop": 350.0,
        "stepsize": 0.2
      }
      ]},
      { "scan_object": "top_helicity_selection",
        "export_type": "separate",
        "ranges": [{ "type": "explicit", "range": [-1,0,1,2,3]} ]
      }
      ],
    "adaption_iterations": "5:10000",
    "integration_iterations": "2:10000",
    "integration_copies": 2
  }
  ],
  "whizard": "whizard",
  "data_output": "./"
}
