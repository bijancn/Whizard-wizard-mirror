include("../../general_settings.sin")
include("../process_settings.sin")
alpha_power = 6
alphas_power = 0

?unweighted = false
?nlo_fixed_order = true
?combined_nlo_integration = true
$loop_me_method = "openloops"

process bbllnn_nlo  = E1, e1 => b, B, E1, n1, e2, N2 {nlo_calculation="Full"}

#Default parameters for FKS mapping
fks_dij_exp1 = 1.0
fks_mapping_type = 1

n_events = 20000 * K
checkpoint = n_events / 20

simulate (bbllnn_nlo)
