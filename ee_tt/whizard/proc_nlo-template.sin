include("../../general_settings.sin")
include("../process_settings.sin")

wtop = 0.0
wZ = 2.5060
wW = 2.0978
real dummy = 0.0
?nlo_fixed_order = true
?unweighted = false
?negative_weights = true
scale = mtop
#ITERATIONS
library = proc_nlo
cuts = all Pt > 20 GeV [T]
process proc_nlo = e1, E1 => t, T { nlo_calculation = "Full" }
#SETSCAN
integrate (proc_nlo)
printf "RES %8e %8e %8e" (mtop, integral(proc_nlo), error(proc_nlo))
