include("../process_settings_threshold.sin")

library = factorized_decayonshellprojected_lo
process factorized_decayonshellprojected_lo = e1, E1 => Wp, Wm, b, B {$restrictions = "3+5~t && 4+6~tbar"}
compile

FF = 9        # tree level from signal comp
offshell_strategy = 2   ! factorized
                    +128  ! decayonshell projected

show(model)

#ITERATIONS
#SETSCAN
sqrtsstepsize = 0.1 GeV
sqrtsmin = sqrts - sqrtsstepsize
sqrtsmax = sqrts + sqrtsstepsize

integrate (factorized_decayonshellprojected_lo)
printf "RES %g %g %g" (sqrts, integral(factorized_decayonshellprojected_lo), error(factorized_decayonshellprojected_lo))
