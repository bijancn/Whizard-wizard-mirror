sqrts = 10000 GeV
library = "unity10000"
$method = "template_unity"
process unity10000 = e1, E1 => Wp, Wm, b, B
compile

?vis_channels = false
?use_vamp_equivalences = false
?vis_history = false
?vis_diags = false

show(model)

#ITERATIONS
#SETSCAN

integrate (unity10000)
sample_format = hepmc
?write_raw = false
?unweighted = false
?hepmc_output_cross_section = true
printf "RES %g %g %g" (sqrts, integral(unity10000), error(unity10000))

